{
  "name": "micro-portalen-roles",
  "description": "Mapping of roles for portalen",
  "version": "1.2.0",
  "license": "MIT",
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.4.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "update-ids": "node utils/add-ids-to-hrm.js",
    "verify-ids": "node utils/verify-ids.js",
    "standard-fix": "standard --fix",
    "start": "NODE_ENV=production micro -p 8080",
    "dev": "micro -p 3000",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "roles",
    "portalen",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/telemark/micro-portalen-roles.git"
  },
  "bugs": {
    "url": "http://github.com/telemark/micro-portalen-roles/issues"
  },
  "now": {
    "alias": "roles.portalen.t-fk.win"
  },
  "greenkeeper": {
    "emails": false
  },
  "devDependencies": {
    "coveralls": "2.11.15",
    "nsp": "2.6.2",
    "standard": "8.6.0",
    "tap": "9.0.3"
  },
  "dependencies": {
    "marked": "0.3.6",
    "micro": "6.2.0",
    "tfk-unique-array": "1.0.6"
  }
}
