{
  "name": "micro-portalen-roles",
  "description": "Mapping of roles for portalen",
  "version": "1.5.3",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10.8.0"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "update-ids": "node utils/add-ids-to-hrm.js",
    "verify-ids": "node utils/verify-ids.js",
    "standard-fix": "standard --fix",
    "dev": "micro-dev",
    "start": "micro",
    "now-deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "roles",
    "portalen",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-portalen-roles.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-portalen-roles/issues"
  },
  "homepage": "https://github.com/telemark/micro-portalen-roles#readme",
  "now": {
    "alias": [
      "roles.portalen.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm",
    "engines": {
      "node": ">=10.0.0"
    }
  },
  "devDependencies": {
    "ava": "0.25.0",
    "axios": "0.18.0",
    "coveralls": "3.0.2",
    "micro-dev": "3.0.0",
    "nyc": "13.0.1",
    "standard": "11.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "markdown-it": "8.4.2",
    "micro": "9.3.3",
    "tfk-unique-array": "1.0.11"
  }
}
